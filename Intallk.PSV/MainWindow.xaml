<Window x:Class="Intallk.PSV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Intallk.PSV"
        mc:Ignorable="d"
        Title="Intallk Painting Script Tools" Height="548" Width="1010" Icon="/QQ图片20220716193154.jpg" Background="#FF404048" SizeChanged="Window_SizeChanged">
    <Grid HorizontalAlignment="Left" Width="1000" Margin="0,0,0,0">
        <DockPanel HorizontalAlignment="Left" LastChildFill="False" Width="206" Background="#FF50505A">
            <Label Content="图片列表" Width="180" Height="37" DockPanel.Dock="Top" Foreground="#FFDCD7E6" Margin="10,0,10,0" FontWeight="Bold" Padding="5,10,5,10"/>
            <ListBox x:Name="picList" Width="184" d:ItemsSource="{d:SampleData ItemCount=5}" DockPanel.Dock="Top" Margin="10,0,10,10" Height="497" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFDCDCE6"/>
        </DockPanel>
    </Grid>
</Window>
